<UserControl x:Class="NanoPanel.DigitalPinFlip"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:NanoPanel"
             mc:Ignorable="d" 
             d:DesignHeight="16" d:DesignWidth="16">
    <Grid>
        <Ellipse x:Name="ellipse" Width="16" Height="16" Fill="Tomato" ToolTip="If output enabled click to flip state." Stroke="Black"/>
    </Grid>
</UserControl>
